{
  "name": "withlouis",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @withlouis/* run dev",
    "dev:freelance": "pnpm --filter @withlouis/freelance run dev",
    "dev:portfolio": "pnpm --filter @withlouis/portfolio run dev",
    "dev:blog": "pnpm --filter @withlouis/blog run dev",
    "build": "pnpm --filter @withlouis/* run build",
    "build:freelance": "pnpm --filter @withlouis/freelance run build",
    "build:portfolio": "pnpm --filter @withlouis/portfolio run build",
    "build:blog": "pnpm --filter @withlouis/blog run build",
    "preview": "pnpm --filter @withlouis/* run preview",
    "deploy": "pnpm --filter @withlouis/* run deploy",
    "deploy:freelance": "pnpm --filter @withlouis/freelance run deploy",
    "deploy:portfolio": "pnpm --filter @withlouis/portfolio run deploy",
    "deploy:blog": "pnpm --filter @withlouis/blog run deploy",
    "deploy:linkme": "pnpm --filter @withlouis/linkme run deploy",
    "clean": "pnpm -r exec rm -rf node_modules dist .astro"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "typescript": "^5.7.0"
  }
}